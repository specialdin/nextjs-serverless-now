{
  "version": 2,
  "name": "nextjs-serverless",
  "builds": [
    {
      "src": "www/package.json",
      "use": "@now/next"
    },
    {
      "src": "api/*/*.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/www/$1"
    }
  ]
}
